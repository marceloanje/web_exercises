    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia perguntas</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <h3>Realizar pergunta</h3>
        <hr>
        <form>
            <label>Título</label>
            <input type="text" placeholder="Título" class="form-control">
            <br><label>Descrição</label>
            <textarea placeholder="Descreva sua pergunta aqui" class="form-control"></textarea>
            <br>
            <button class="btn btn-success">Perguntar</button>
        </form>
    </div>
</body>
